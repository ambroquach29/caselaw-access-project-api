{
  "name": "caselaw-access-project-api",
  "version": "1.0.0",
  "description": "A GraphQL API for accessing and managing case law data",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "ts-node-dev server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "seed": "ts-node scripts/seed.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "docker:build": "docker build -t caselaw-api .",
    "docker:run": "docker run -p 5001:5000 caselaw-api",
    "docker:compose": "docker compose up -d",
    "docker:compose:down": "docker compose down",
    "db:generate": "npx prisma generate --schema=database/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=database/prisma/schema.prisma",
    "db:queries": "ts-node db_queries.ts",
    "import:bulk": "ts-node scripts/bulk-import.ts",
    "import:bulk:advanced": "ts-node scripts/bulk_import_adv.ts",
    "extract:sample": "ts-node scripts/sample-extractor.ts",
    "cleanup:duplicates": "ts-node scripts/bulk-import.ts --cleanup"
  },
  "keywords": [
    "graphql",
    "apollo-server",
    "case-law",
    "legal-api",
    "typescript",
    "express"
  ],
  "author": "Ambro Quach",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/server": "^4.10.0",
    "@prisma/client": "^6.10.1",
    "apollo-server-express": "^3.13.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "prisma": "^6.10.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/caselaw-access-project-api.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/caselaw-access-project-api/issues"
  },
  "homepage": "https://github.com/yourusername/caselaw-access-project-api#readme"
}
